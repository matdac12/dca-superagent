# Binance API Keys
# Get from: https://www.binance.com/en/my/settings/api-management
# IMPORTANT: Enable "Spot Trading" and set IP to "Unrestricted" or add your server IP
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here
BINANCE_TESTNET=false

# OpenAI API Key
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-proj-xxxxxxxxxxxxx

# Telegram Notifications (Optional)
# Bot Token: Message @BotFather on Telegram, send /newbot
# Chat ID: Start chat with your bot, then visit:
# https://api.telegram.org/bot<YOUR_BOT_TOKEN>/getUpdates
TELEGRAM_BOT_TOKEN=123456789:ABCdefGHIjklMNOpqrsTUVwxyz
TELEGRAM_CHAT_ID=123456789

# Execution Mode
# true = Simulate orders (safe for testing)
# false = Execute real orders (uses real money!)
DRY_RUN=true
